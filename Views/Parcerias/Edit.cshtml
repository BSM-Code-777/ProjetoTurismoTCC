@model turismoTCC.Models.Parceria

@{
    ViewData["Title"] = "Edit";
}

<h1>Edit</h1>

<h4>Parceria</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Edit">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="idParceria" />
            <div class="form-group">
                <label asp-for="beneficio" class="control-label"></label>
                <input asp-for="beneficio" class="form-control" />
                <span asp-validation-for="beneficio" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="idLocal" class="control-label"></label>
                <select asp-for="idLocal" class="form-control" asp-items="ViewBag.idLocal"></select>
                <span asp-validation-for="idLocal" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="idPremium" class="control-label"></label>
                <select asp-for="idPremium" class="form-control" asp-items="ViewBag.idPremium"></select>
                <span asp-validation-for="idPremium" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Save" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Back to List</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
